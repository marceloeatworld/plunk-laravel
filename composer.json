{
    "name": "MarceloEatWorld/plunk-laravel",
    "description": "Laravel mail transport for Plunk API",
    "keywords": ["laravel", "plunk", "mail", "email", "transport"],
    "license": "MIT",
    "authors": [
        {
            "name": "marcelo",
            "email": "diagngo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/mail": "^8.0|^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "MarceloEatWorld\\PlunkLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarceloEatWorld\\PlunkLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarceloEatWorld\\PlunkLaravel\\PlunkServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}